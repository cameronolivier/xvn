{
  "name": "@olvrcc/xvn",
  "version": "1.6.0",
  "description": "Automatic project Node version switching",
  "main": "install.js",
  "bin": {
    "xvn": "bin/xvn"
  },
  "scripts": {
    "postinstall": "node install.js",
    "preuninstall": "node uninstall.js",
    "build": "cargo build --release",
    "test": "cargo test",
    "dev": "cargo build && cargo install --path .",
    "setup": "npm run dev && xvn setup",
    "lint": "cargo clippy -- -D warnings",
    "fmt": "cargo fmt",
    "check": "cargo fmt -- --check && cargo clippy -- -D warnings && cargo test",
    "release:download": "./scripts/download-artifacts.sh",
    "release:extract": "./scripts/extract-binaries.sh",
    "release:pack": "npm pack",
    "release:verify": "tar -tzf $(ls -t *.tgz | head -1) | grep -E '(native|shell)'"
  },
  "keywords": [
    "node",
    "version",
    "nvm",
    "fnm",
    "automatic",
    "switching",
    "cli"
  ],
  "author": "cameronolivier@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olvrcc/xvn.git"
  },
  "bugs": {
    "url": "https://github.com/olvrcc/xvn/issues"
  },
  "homepage": "https://github.com/olvrcc/xvn#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "files": [
    "bin/",
    "native/",
    "shell/",
    "install.js",
    "uninstall.js"
  ]
}
