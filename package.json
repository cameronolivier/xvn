{
  "name": "@olvrcc/xvn",
  "version": "1.1.1",
  "description": "Automatic Node.js version switching for cd",
  "main": "install.js",
  "bin": {
    "xvn": "./bin/xvn"
  },
  "scripts": {
    "postinstall": "node install.js",
    "build": "cargo build --release",
    "test": "cargo test",
    "dev": "cargo build && cargo install --path .",
    "setup": "npm run dev && xvn setup",
    "lint": "cargo clippy -- -D warnings",
    "fmt": "cargo fmt",
    "check": "cargo fmt -- --check && cargo clippy -- -D warnings && cargo test"
  },
  "keywords": [
    "node",
    "version",
    "nvm",
    "fnm",
    "automatic",
    "switching",
    "cli"
  ],
  "author": "cameronolivier@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cameronolivier/xvn.git"
  },
  "bugs": {
    "url": "https://github.com/cameronolivier/xvn/issues"
  },
  "homepage": "https://github.com/cameronolivier/xvn#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "files": [
    "bin/",
    "native/",
    "install.js"
  ]
}
